/*Table structure for table `matching` */

DROP TABLE IF EXISTS `matching`;

CREATE TABLE `matching` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT,
  `requestid` int(11) unsigned NOT NULL,
  `userid` int(11) unsigned NOT NULL,
  `providerid` int(11) unsigned NOT NULL,
  `rating` int(11) NOT NULL,
  `ratingdetail` text COLLATE utf8_unicode_ci NOT NULL,
  `clientmatching` int(11) NOT NULL,
  PRIMARY KEY (`id`),
  KEY `requestid` (`requestid`),
  KEY `providerid` (`providerid`),
  KEY `userid` (`userid`),
  CONSTRAINT `matching_ibfk_1` FOREIGN KEY (`requestid`) REFERENCES `requests` (`requestid`) ON DELETE CASCADE ON UPDATE CASCADE,
  CONSTRAINT `matching_ibfk_2` FOREIGN KEY (`userid`) REFERENCES `users` (`userid`) ON DELETE CASCADE ON UPDATE CASCADE,
  CONSTRAINT `matching_ibfk_3` FOREIGN KEY (`providerid`) REFERENCES `providers` (`providerid`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci;

/*Data for the table `matching` */

/*Table structure for table `provider_ledger` */

DROP TABLE IF EXISTS `provider_ledger`;

CREATE TABLE `provider_ledger` (
  `ledgerid` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `transactiontype` enum('Purchase','Lead','Bonus') CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL,
  `recordid` int(11) NOT NULL DEFAULT '0',
  `transactiondate` datetime NOT NULL DEFAULT '0000-00-00 00:00:00',
  `providerid` int(11) NOT NULL DEFAULT '0',
  `amount` decimal(12,4) NOT NULL DEFAULT '0.0000',
  `status` enum('Plus','Minus') CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL,
  PRIMARY KEY (`ledgerid`)
