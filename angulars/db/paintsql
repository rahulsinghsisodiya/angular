/*Table structure for table `alerts` */

DROP TABLE IF EXISTS `alerts`;

CREATE TABLE `alerts` (
  `alertid` int(11) NOT NULL AUTO_INCREMENT,
  `type` varchar(32) COLLATE utf8_unicode_ci NOT NULL,
  `providerid` int(11) unsigned NOT NULL,
  `userid` int(11) unsigned NOT NULL,
  `message` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `status` int(11) NOT NULL DEFAULT '0',
  PRIMARY KEY (`alertid`),
  KEY `providerid` (`providerid`),
  KEY `userid` (`userid`),
  CONSTRAINT `alerts_ibfk_1` FOREIGN KEY (`providerid`) REFERENCES `providers` (`providerid`) ON DELETE CASCADE ON UPDATE CASCADE,
  CONSTRAINT `alerts_ibfk_2` FOREIGN KEY (`userid`) REFERENCES `users` (`userid`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci;

/*Data for the table `alerts` */

/*Table structure for table `bid_percent` */

DROP TABLE IF EXISTS `bid_percent`;

