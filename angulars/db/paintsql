/*Table structure for table `response` */

DROP TABLE IF EXISTS `response`;

CREATE TABLE `response` (
  `responseid` int(11) unsigned NOT NULL AUTO_INCREMENT,
  `responseno` decimal(8,0) DEFAULT NULL,
