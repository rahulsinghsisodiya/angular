/*Table structure for table `cities` */

DROP TABLE IF EXISTS `cities`;

CREATE TABLE `cities` (
  `cityid` mediumint(7) NOT NULL AUTO_INCREMENT,
  `cityname` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `postalcode` varchar(5) COLLATE utf8_unicode_ci NOT NULL,
  `regionid` mediumint(7) NOT NULL,
  `insee_id` varchar(5) COLLATE utf8_unicode_ci NOT NULL,
  PRIMARY KEY (`cityid`),
  KEY `fk_cities_1_idx` (`regionid`),
  CONSTRAINT `FK1` FOREIGN KEY (`regionid`) REFERENCES `regions` (`regionid`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB AUTO_INCREMENT=48 DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci;

